package a;

import java.io.*;
import java.util.Scanner;

public class Study {
	
	public static void main(String[] args) {
		
		try {
			
			BufferedReader br = new BufferedReader( new InputStreamReader(System.in) );
			
			System.out.println(">");
			
			String name = br.readLine();
			
			System.out.println(name);
			
			br.close();
			
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		
	}
  
}
