package a;

import java.io.FileOutputStream;
import java.io.OutputStream;

public class Study {
	
	public static void main(String[] args) {
		
		String path = "C:\\Users\\user\\Desktop\\course\\Java\\file\\ex1.txt";
		
		try {
			OutputStream out = new FileOutputStream(path);
			
			out.write(66);
			out.write(66);
			out.write(66);
			out.write(66);
			
			byte[] arr = {70,71,72,73,75};
			out.write(arr);
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		
		
	}
	
	
        
  
}
